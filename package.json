    "@socket.io/sticky": "^1.0.4",
    "dotenv": "^17.2.3",
    "express": "^4.17.3",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.21",
    "redis": "^5.8.3",
    "shortid": "^2.2.16",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "throttle-debounce": "^2.3.0",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.2",
