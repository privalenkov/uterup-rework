{
  "name": "jumpking-io-game",
  "version": "1.0.0",
  "description": "Jump King style multiplayer .io game",
  "main": "src/server/server.js",
  "scripts": {
    "develop": "NODE_ENV=development node src/server/server.js",
    "develop:cluster": "NODE_ENV=development node server-cluster.js",
    "build": "webpack --config webpack.prod.js",
